{
    
        "require": {
          "php": "^8.1",
          "doctrine/orm": "^3.0",
        "doctrine/annotations": "^1.13",
          "doctrine/dbal": "^3.0",
          "doctrine/collections": "^1.6",
          "doctrine/persistence": "^3.0",
          "doctrine/cache": "^2.2",
          "doctrine/instantiator": "^1.3"
        },
      
      
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  }
}
